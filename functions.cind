global number first = 3;
global number second = 5;

bool isBoth(number i){
    return i % first == 0 && i % second == 0;
}

void printCell(number n, string delimiter, building cell, building message){
    for(number i = 0; i < n; i++){
        print(cell[i]);
        if(i + 1 != n)
            print(delimiter);
    }

    printFlush(message);
}
